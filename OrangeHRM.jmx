            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import javax.crypto.Cipher;
import javax.crypto.spec.SecretKeySpec;
import javax.crypto.spec.IvParameterSpec;
import org.apache.commons.codec.binary.Base64;

// --- AES Configuration ---
// This must be the SAME secret key and IV used for encryption.
def secretKeyString = &quot;1234567890123456&quot;;
def encryptionAlgorithm = &quot;AES/CBC/PKCS5Padding&quot;;
def ivBytes = new byte[16]; // Must be the SAME IV as used for encryption
def ivSpec = new IvParameterSpec(ivBytes);

// Get the encrypted password from the JMeter variable
def encryptedPasswordBase64 = vars.get(&quot;encryptedPasswordVar&quot;);

if (encryptedPasswordBase64 == null) {
    log.warn(&quot;encryptedPasswordVar is null. No password to decrypt.&quot;);
    return;
}

try {
    // Create the SecretKeySpec from your secret key string bytes
    SecretKeySpec keySpec = new SecretKeySpec(secretKeyString.getBytes(&quot;UTF-8&quot;), &quot;AES&quot;);

    // Initialize the Cipher for DECRYPT_MODE with the key and IV
    Cipher cipher = Cipher.getInstance(encryptionAlgorithm);
    cipher.init(Cipher.DECRYPT_MODE, keySpec, ivSpec);

    // Base64 decode the encrypted string back to bytes
    byte[] encryptedBytes = Base64.decodeBase64(encryptedPasswordBase64);

    // Perform decryption
    byte[] decryptedBytes = cipher.doFinal(encryptedBytes);

    // Convert decrypted bytes back to a UTF-8 string
    String decryptedPassword = new String(decryptedBytes, &quot;UTF-8&quot;);

    // Store the decrypted password in a new JMeter variable for use in the sampler
    vars.put(&quot;decryptedPasswordVar&quot;, decryptedPassword);
    log.info(&quot;AES Decrypted Password: &quot; + decryptedPassword);

} catch (Exception e) {
    log.error(&quot;AES Decryption failed: &quot; + e.getMessage());
    throw e; // Re-throw to fail the sampler if decryption is critical
}</stringProp>
          </JSR223PreProcessor>
