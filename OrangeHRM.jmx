              <stringProp name="cacheKey">true</stringProp>
              <stringProp name="script">import javax.crypto.Cipher;
import javax.crypto.spec.SecretKeySpec;
import javax.crypto.spec.IvParameterSpec;
import org.apache.commons.codec.binary.Base64;

// --- AES Configuration ---
// Your specified secret key (16 characters = 16 bytes for AES-128)
def secretKeyString = &quot;1234567890123456&quot;;
def encryptionAlgorithm = &quot;AES/CBC/PKCS5Padding&quot;;
def ivBytes = new byte[16]; // Fixed IV for simplicity in JMeter variables
def ivSpec = new IvParameterSpec(ivBytes);

// Get the plain text password from the JMeter variable
def plainTextPassword = &quot;admin123&quot; // This will be &quot;admin&quot;

if (plainTextPassword == null) {
    log.warn(&quot;plainTextPasswordVar is null. No password to encrypt.&quot;);
    return;
}

try {
    // Create the SecretKeySpec from your secret key string bytes
    SecretKeySpec keySpec = new SecretKeySpec(secretKeyString.getBytes(&quot;UTF-8&quot;), &quot;AES&quot;);

    // Initialize the Cipher for ENCRYPT_MODE with the key and IV
    Cipher cipher = Cipher.getInstance(encryptionAlgorithm);
    cipher.init(Cipher.ENCRYPT_MODE, keySpec, ivSpec);

    // Perform encryption
    byte[] encryptedBytes = cipher.doFinal(plainTextPassword.getBytes(&quot;UTF-8&quot;));

    // Base64 encode the encrypted bytes for safe storage/transmission
    String encryptedPasswordBase64 = Base64.encodeBase64String(encryptedBytes);

    // Store the encrypted password in a new JMeter variable
    vars.put(&quot;encryptedPasswordVar&quot;, encryptedPasswordBase64);
    log.info(&quot;AES Encrypted Password for &apos;admin123&apos;: &quot; + encryptedPasswordBase64);

} catch (Exception e) {
    log.error(&quot;AES Encryption failed: &quot; + e.getMessage());
    throw e; // Re-throw to fail the sampler if encryption is critical
}</stringProp>
