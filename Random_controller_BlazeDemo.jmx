            <stringProp name="script">import org.jsoup.Jsoup
import java.util.Random

// Get the HTML from previous sampler (reserve.php)
def html = prev.getResponseDataAsString()

// Parse HTML using Jsoup
def doc = Jsoup.parse(html)

// Select all flight rows (ignoring the header row)
def rows = doc.select(&quot;table tr:has(td)&quot;)

if (rows.isEmpty()) {
    log.warn(&quot;No flight rows found!&quot;)
} else {
    // Generate a random index to select a random flight
    def random = new Random()
    def randomIndex = random.nextInt(rows.size())
    def selectedRow = rows[randomIndex]

    def cells = selectedRow.select(&quot;td&quot;)
    def flightNo = cells[1].text()
    def airline = cells[2].text()
    def priceText = cells[5].text().replace(&apos;$&apos;, &apos;&apos;).trim()

    // Save chosen flight details as JMeter variables
    vars.put(&quot;flight&quot;, flightNo)
    vars.put(&quot;airline&quot;, airline)
    vars.put(&quot;price&quot;, priceText)

    log.info(&quot;Chosen Random Flight → Flight#: ${flightNo}, Airline: ${airline}, Price: ${priceText}&quot;)
}
</stringProp>
