int start = response.indexOf(&quot;Flights from &quot;) + &quot;Flights from &quot;.length();
int end = response.indexOf(&quot;:&quot;, start);
String route = response.substring(start, end);
log.info(&quot;Route: &quot; + route);

Pattern pattern = Pattern.compile(&quot;value=\&quot;(\\d+)\&quot; name=\&quot;flight\&quot;&quot;);
// In this case, matcher.group(1) will directly give you &quot;43&quot;.
Matcher matcher = pattern.matcher(response);
if (matcher.find()) { // Corrected: added &apos;)&apos;
    String flightId = matcher.group(1); // It&apos;s good practice to assign it to a local variable first if you use it multiple times
    vars.put(&quot;flightId&quot;, flightId);
    log.info(&quot;Extracted Flight Id: &quot; + flightId); // Corrected: using the local variable
}</stringProp>
