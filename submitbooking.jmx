          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import org.jsoup.Jsoup
import org.jsoup.nodes.Document
import org.jsoup.select.Elements

// Get HTML from previous sampler response
String html = prev.getResponseDataAsString()

// Parse HTML with Jsoup
Document doc = Jsoup.parse(html)

// Select all table rows inside tbody (skip header)
Elements rows = doc.select(&quot;table.table tbody tr&quot;)

def cheapestFlight = null
float minPrice = Float.MAX_VALUE

rows.each { row -&gt;
    Elements cols = row.select(&quot;td&quot;)

    // Expecting at least 6 columns (first is button)
    if (cols.size() &gt;= 6) {
        try {
            // Extract values by index based on your HTML
            String flightNo = cols[1].text().trim()       // Flight #
            String airline = cols[2].text().trim()        // Airline
            String departure = cols[3].text().trim()      // Departure time
            String arrival = cols[4].text().trim()        // Arrival time
            String priceText = cols[5].text().replace(&apos;$&apos;, &apos;&apos;).trim()  // Price without &apos;$&apos;
            float price = Float.parseFloat(priceText)

            if (price &lt; minPrice) {
                minPrice = price
                cheapestFlight = [
                    flightNo : flightNo,
                    airline  : airline,
                    departure: departure,
                    arrival  : arrival,
                    price    : price
                ]
            }
        } catch (Exception e) {
            log.warn(&quot;Skipping row due to parse error: &quot; + e.message)
        }
    }
}

// Save cheapest flight details to JMeter variables if found
if (cheapestFlight != null) {
    vars.put(&apos;cheapestFlightNumber&apos;, cheapestFlight.flightNo)
    vars.put(&apos;cheapestAirline&apos;, cheapestFlight.airline)
    vars.put(&apos;cheapestDeparture&apos;, cheapestFlight.departure)
    vars.put(&apos;cheapestArrival&apos;, cheapestFlight.arrival)
    vars.put(&apos;cheapestPrice&apos;, cheapestFlight.price.toString())

    log.info(&quot;✅ Cheapest flight selected:&quot;)
    log.info(&quot;Flight Number: ${cheapestFlight.flightNo}&quot;)
    log.info(&quot;Airline      : ${cheapestFlight.airline}&quot;)
    log.info(&quot;Departure    : ${cheapestFlight.departure}&quot;)
    log.info(&quot;Arrival      : ${cheapestFlight.arrival}&quot;)
    log.info(&quot;Price        : ${cheapestFlight.price}&quot;)
} else {
    log.error(&quot;❌ No valid flights found in response!&quot;)
}</stringProp>
          </JSR223PreProcessor>
