              <boolProp name="resetInterpreter">false</boolProp>
              <stringProp name="script">class BusinessException extends Exception {
    public BusinessException(String message) {
        super(message);
    }
}
try {
    String responseBody = prev.getResponseDataAsString();
    
    if (responseBody == null) {
        throw new Exception(&quot;Response body is null&quot;);
    }
    
    if (responseBody.contains(&quot;The specified email already exists&quot;)) {
        throw new BusinessException(&quot;Email already exists&quot;); // Throw custom exception
    } else {
        log.info(&quot;Registration proceeded. No &apos;email already exists&apos; error detected.&quot;);
    }
} catch (BusinessException e) {
    log.warn(&quot;Business validation failed: &quot; + e.getMessage());
    ctx.getThread().stop(); 
} catch (Exception e) {
    log.error(&quot;Technical error: &quot; + e.getMessage(), e);
}</stringProp>
