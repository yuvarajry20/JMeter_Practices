              <stringProp name="filename"></stringProp>
              <stringProp name="script">String response = prev.getResponseDataAsString();
String expectedCity = vars.get(&quot;destinationCity&quot;);
if (response.contains(expectedCity)) {
    log.info(&quot;Destination city validated successfully: &quot; + expectedCity);
} else {
    log.error(&quot;Destination city NOT found in response. Expected: &quot; + expectedCity);
}

import java.util.regex.*;

String response = prev.getResponseDataAsString();
String pattern = &quot;&lt;input type=\&quot;hidden\&quot; value=\&quot;(\\d+)\&quot; name=\&quot;flight\&quot;&gt;&quot;;
Pattern r = Pattern.compile(pattern);
Matcher m = r.matcher(response);

int count = 0;
while (m.find()) {
    count++;
    log.info(&quot;✅ Flight ID found: &quot; + m.group(1));
}

vars.put(&quot;flightCount&quot;, Integer.toString(count));
log.info(&quot;✈️ Total flights listed: &quot; + count);


int random = 1 + (int)(Math.random() * Integer.parseInt(vars.get(&quot;flightID_matchNr&quot;)));
vars.put(&quot;selectedFlight&quot;, vars.get(&quot;flightID_&quot; + random));
vars.put(&quot;selectedPrice&quot;, vars.get(&quot;price_&quot; + random));
vars.put(&quot;selectedAirline&quot;, vars.get(&quot;airline_&quot; + random));


</stringProp>
